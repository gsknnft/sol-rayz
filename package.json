{
  "name": "@nfteyez/root",
  "description": "Set of utils to parse NFTs on Solana blockchain.",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "docs": "doctoc --title '**Table of content**' README.md",
    "clean": "lerna run clean",
    "clean:deps": "find . -name 'node_modules' -type d -prune -exec rm -rf '{}' +",
    "build": "lerna run build",
    "watch": "lerna run watch",
    "publish": "lerna publish",
    "test": "lerna run test",
    "lint": "eslint --ext js,ts,tsx ."
  },
  "devDependencies": {
    "@nighttrax/eslint-config-tsx": "~12.0.0-alpha.3",
    "doctoc": "~2.2.1",
    "eslint": "~8.48.0",
    "eslint-plugin-import": "~2.25.0",
    "lerna": "~4.0.0",
    "react": "18.2.0",
    "typescript": "~5.2.2"
  }
}
